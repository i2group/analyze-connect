{
  "name": "nodeconnector",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "nodemon ./bin/www",
    "prettier-fix": "prettier --write .helpers/*.js .routes/*.js ./*.js",
    "prettier-check": "prettier --check .helpers/*.js .routes/*.js ./*.js",
    "test": "mocha --recursive test",
    "test:coverage": "nyc npm test",
    "test:unit": "mocha --recursive test/middleware test/models test/routes",
    "test:integration": "mocha --recursive test/integration"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "body-parser": "^1.13.3",
    "cookie-parser": "^1.3.3",
    "express": "^4.13.3",
    "morgan": "^1.6.1"
  },
  "devDependencies": {
    "debug": "^2.2.0",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-develop": "^0.4.0",
    "load-grunt-tasks": "^3.2.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.19.4",
    "nyc": "^10.0.0",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "supertest": "^2.0.0",
    "time-grunt": "^1.2.1"
  }
}
